{
  "configurations": [
    {
      "browse": {
        "databaseFilename": "${default}",
        "limitSymbolsToIncludedHeaders": false
      },
      "includePath": [
        "${workspaceFolder}/**",
        "/usr/include/**",
        "/opt/ros/noetic/include/**"
      ],
      "name": "linux-gcc-x64",
      "intelliSenseMode": "linux-gcc-x64",
      "compilerPath": "gcc",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "compilerArgs": [
        "-Wall",
        "-Wextra",
        "-Wpedantic"
      ]
    }
  ],
  "version": 4
}